  "compiler_std": "c++20",
  "headers_path": [
    "./dev/ToolchainKit",
    "./dev",
    "./dev/ToolchainKit/src/Detail"
  ],
  "sources_path": ["dev/ToolchainKit/src/*.cxx"],
